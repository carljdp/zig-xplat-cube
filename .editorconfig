# EditorConfig docs: https://EditorConfig.org

# top-most EditorConfig file
root = true

# For * (any) file
[*]
indent_style = space
indent_size = 2
tab_width = 4
charset = utf-8
insert_final_newline = true

# end_of_line ? Delegated to .gitattributes
# trim_trailing_whitespace ? Meh, sometimes it's useful
